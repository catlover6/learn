
class stack:
    def __init__(self):
        self.top = []

    def isEmpty(self):
        return len(self.top)==0

    def push(self,item):
        self.top.append(item)

    def pop(self):
        if not self.isEmpty():
            return self.top.pop(-1)

    def peek(self):
        if not self.isEmpty():
            return self.top[-1]

    def size(self): return len(self.top)

    def clear(self):
        top = []

    def show(self):
        print(self.top)

a = stack()

for i in range(1,6):
    a.push(i)

for i in range (5):
    print(a.top)
